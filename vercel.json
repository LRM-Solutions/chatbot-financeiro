{
  "version": 2,
  "builds": [
    {
      "src": "main.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/schema.prisma"],
        "scripts": {
          "build": "npm run build" 
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.js"
    }
  ]
}